<h1>
    <%= title %>
</h1>
<div id="all-users-container" class="user-list">
    <ul>
        <% for (let i = 0; i < range; i++) {%>
            <li><p class="user-item"><a href="/users/profile/<%= users[i].id %>"><%= users[i].name %></a></p></li>
        <% } %>
    </ul>
</div>


<div id="pages">         
    <% for (let i = pageNo-2; i <= pageNo+2 ; i++) {%>
        <% if (i != -1 && i != 0){%>
            <a href="/users/allUsers/?pageNo=<%= i %>" style= "
                <%= i === pageNo && users_length>7? 'font-weight: bold; background-color: #2196f3; color: #fff;' : '' %>
                border-radius: 50%;
                width: 40px;
                height: 40px;
                line-height: 60px;
                text-align: center;
                margin: 0 10px;
              ":hover>
                <% if (i<pageNo) {%>
                    <%= i %> 
                <% } else if (i==pageNo && users_length>7){%>
                    <%= i %> 
                <%} else if (i==pageNo+1 && users_length>7){%>
                    <%= i %> 
                <%} else if (i==pageNo+2 && users_length>14){%>
                    <%= i %> 
                <%}%>
            </a> &nbsp;&nbsp;&nbsp;&nbsp;
        <% } %>


    <% } %>
</div>